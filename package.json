{
  "name": "work-log-ify",
  "version": "1.22.2",
  "description": "WorkLogIfy",
  "main": "dist-electron/main/index.js",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder --windows --publish never",
    "standard-version": "npx standard-version",
    "standard-version-minor": "npx standard-version --release-as minor",
    "standard-version-patch": "npx standard-version --release-as patch",
    "standard-version-prerelease": "npx standard-version --prerelease local --release-as patch --skip.tag",
    "lint": "tslint app/**/*.ts{,x}"
  },
  "author": "Dmitrijs Sadovskis",
  "license": "MIT",
  "standard-version": {
    "header": "# Changelog\n\nAll notable changes to this project will be documented in this file.\n",
    "types": [
      {
        "type": "feat",
        "section": "Features"
      },
      {
        "type": "patch",
        "section": "Bug Fixes"
      },
      {
        "type": "ux",
        "section": "UI/UX"
      }
    ],
    "releaseCommitMessageFormat": "chore(release): v{{currentTag}} [ci skip]"
  },
  "dependencies": {
    "@electron/remote": "^2.0.10",
    "electron-is-dev": "^1.2.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "@babel/types": "*",
    "@types/lodash": "*",
    "@types/node": "*",
    "@vitejs/plugin-vue": "^4.2.3",
    "direct-vuex": "^1.0.0-rc3",
    "electron": "^25.0",
    "electron-builder": "^24.0",
    "immutable": "^3.8.2",
    "sass": "^1.26.3",
    "standard-version": "^9.3.2",
    "tslint": "^5.20.1",
    "typescript": "^5.0",
    "vite": "^4.4.4",
    "vite-plugin-electron": "^0.12.0",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.3.4",
    "vue-facing-decorator": "^2.1.20",
    "vue-tsc": "^1.8.5",
    "vuex": "^4.1.0",
    "lodash": "^4.17.21"
  }
}
